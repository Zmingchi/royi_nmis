    // 添加获取某天数据的方法
    public ScheDetail getDayData(int dayIndex) {
        switch (dayIndex) {
            case 1: return day1;
            case 2: return day2;
            case 3: return day3;
            case 4: return day4;
            case 5: return day5;
            case 6: return day6;
            case 7: return day7;
            default: throw new IllegalArgumentException("无效的dayType: " + dayIndex);
        }
    }


    // 便捷方法：根据dayType设置对应天的数据
    public void setDayData(String dayType, ScheDetail dayData) {
        switch (dayType) {
            case "day1": setDay1(dayData); break;
            case "day2": setDay2(dayData); break;
            case "day3": setDay3(dayData); break;
            case "day4": setDay4(dayData); break;
            case "day5": setDay5(dayData); break;
            case "day6": setDay6(dayData); break;
            case "day7": setDay7(dayData); break;
            default: throw new IllegalArgumentException("无效的dayType: " + dayType);
        }
    }


