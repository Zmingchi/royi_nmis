    <sql id="selectNmisHrCareerVo">
        select id, nurse_id, nurse_name, work_unit, duty, begin_date, end_date, title, remark, del_flag, revision, create_id, creator, create_date, modify_id, modifier, modify_date, unit_code from nmis.nmis_hr_career
    </sql>

    <select id="selectNmisHrCareerList" parameterType="NmisHrCareer" resultMap="NmisHrCareerResult">
        <include refid="selectNmisHrCareerVo"/>
        <where>
            <if test="nurseId != null "> and nurse_id = #{nurseId}</if>
            <if test="nurseName != null  and nurseName != ''"> and nurse_name like CONCAT(concat('%', #{nurseName}), '%')</if>
            <if test="workUnit != null  and workUnit != ''"> and work_unit = #{workUnit}</if>
            <if test="duty != null  and duty != ''"> and duty = #{duty}</if>
            <if test="beginDate != null "> and begin_date = #{beginDate}</if>
            <if test="endDate != null "> and end_date = #{endDate}</if>
