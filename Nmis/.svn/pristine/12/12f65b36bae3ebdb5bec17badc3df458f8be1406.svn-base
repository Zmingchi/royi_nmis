    <sql id="selectNmisHrExpatriateVo">
        select id, nurse_id, nurse_name, src_dept_id, src_dept_name, expatriate_unit, expatriate_type, begin_date, end_date, expatriate_reason, remark, del_flag, revision, create_id, creator, create_date, modify_id, modifier, modify_date, unit_code from nmis.nmis_hr_expatriate
    </sql>

    <select id="selectNmisHrExpatriateList" parameterType="NmisHrExpatriate" resultMap="NmisHrExpatriateResult">
        <include refid="selectNmisHrExpatriateVo"/>
        <where>
            <if test="nurseId != null "> and nurse_id = #{nurseId}</if>
            <if test="nurseName != null  and nurseName != ''"> and nurse_name like CONCAT(concat('%', #{nurseName}), '%')</if>
            <if test="srcDeptId != null  and srcDeptId != ''"> and src_dept_id = #{srcDeptId}</if>
            <if test="srcDeptName != null  and srcDeptName != ''"> and src_dept_name like CONCAT(concat('%', #{srcDeptName}), '%')</if>
            <if test="expatriateUnit != null  and expatriateUnit != ''"> and expatriate_unit = #{expatriateUnit}</if>
            <if test="expatriateType != null  and expatriateType != ''"> and expatriate_type = #{expatriateType}</if>
            <if test="beginDate != null "> and begin_date = #{beginDate}</if>
            <if test="endDate != null "> and end_date = #{endDate}</if>
            <if test="expatriateReason != null  and expatriateReason != ''"> and expatriate_reason = #{expatriateReason}</if>
            <if test="revision != null "> and revision = #{revision}</if>
            <if test="createId != null  and createId != ''"> and create_id = #{createId}</if>
            <if test="creator != null  and creator != ''"> and creator = #{creator}</if>
            <if test="createDate != null "> and create_date = #{createDate}</if>
            <if test="modifyId != null  and modifyId != ''"> and modify_id = #{modifyId}</if>
            <if test="modifier != null  and modifier != ''"> and modifier = #{modifier}</if>
            <if test="modifyDate != null "> and modify_date = #{modifyDate}</if>
            <if test="unitCode != null  and unitCode != ''"> and unit_code = #{unitCode}</if>
        </where>
    </select>

    <select id="selectNmisHrExpatriateById" parameterType="Long" resultMap="NmisHrExpatriateResult">
        <include refid="selectNmisHrExpatriateVo"/>
        where id = #{id}
    </select>

    <insert id="insertNmisHrExpatriate" parameterType="NmisHrExpatriate" useGeneratedKeys="true" keyProperty="id">

        <selectKey keyProperty="id" order="BEFORE" resultType="Long">
            select nmis.seq_hr_expatriate.nextval as id from DUAL
        </selectKey>

        insert into nmis.nmis_hr_expatriate
