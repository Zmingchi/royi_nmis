            <if test="unitCode != null">unit_code,</if>
        </trim>
        <trim prefix="values (" suffix=")" suffixOverrides=",">
            <if test="id != null">#{id},</if>
            <if test="nurseId != null">#{nurseId},</if>
            <if test="nurseName != null">#{nurseName},</if>
