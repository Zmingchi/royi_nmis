            <if test="nurseId != null">nurse_id,</if>
            <if test="nurseName != null">nurse_name,</if>
            <if test="srcDeptId != null">src_dept_id,</if>
            <if test="srcDeptName != null">src_dept_name,</if>
            <if test="expatriateUnit != null">expatriate_unit,</if>
