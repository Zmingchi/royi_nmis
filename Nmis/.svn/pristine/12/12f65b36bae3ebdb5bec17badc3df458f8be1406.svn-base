            modify_date = sysdate,
            <if test="unitCode != null">unit_code = #{unitCode},</if>
        </trim>
        where id = #{id}
    </update>

    <delete id="deleteNmisHrExpatriateById" parameterType="Long">
        delete from nmis.nmis_hr_expatriate where id = #{id}
    </delete>

