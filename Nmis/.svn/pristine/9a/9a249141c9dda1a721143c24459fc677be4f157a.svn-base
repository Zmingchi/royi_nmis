            case "day2": return getDay2();
            case "day3": return getDay3();
            case "day4": return getDay4();
            case "day5": return getDay5();
            case "day6": return getDay6();
            case "day7": return getDay7();
            default: throw new IllegalArgumentException("无效的dayType: " + dayType);
        }
    }

