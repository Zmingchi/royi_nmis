package com.ydl.nmis.schedule.dto;

import com.ydl.common.core.domain.BaseEntity;

public class Member extends BaseEntity {
    private String id;
    private String name;
    private String sNo;
    private String nj;
    private String workYear;
    private String memo;

    // 使用固定字段替代Map
    private ScheDayData day1;
    private ScheDayData day2;
    private ScheDayData day3;
    private ScheDayData day4;
    private ScheDayData day5;
    private ScheDayData day6;
    private ScheDayData day7;


    // 便捷方法：根据dayType设置对应天的数据
    public void setDayData(String dayType, ScheDayData dayData) {
        switch (dayType) {
            case "day1": setDay1(dayData); break;
            case "day2": setDay2(dayData); break;
            case "day3": setDay3(dayData); break;
            case "day4": setDay4(dayData); break;
            case "day5": setDay5(dayData); break;
            case "day6": setDay6(dayData); break;
            case "day7": setDay7(dayData); break;
            default: throw new IllegalArgumentException("无效的dayType: " + dayType);
        }
    }
    // getters and setters...


    public String getId() {
        return id;
    }

    public void setId(String id) {
        this.id = id;
    }

    public String getName() {
        return name;
    }

    public void setName(String name) {
        this.name = name;
    }

    public String getsNo() {
        return sNo;
    }

    public void setsNo(String sNo) {
        this.sNo = sNo;
    }

    public String getNj() {
        return nj;
    }

    public void setNj(String nj) {
        this.nj = nj;
    }

    public String getWorkYear() {
        return workYear;
    }

    public void setWorkYear(String workYear) {
        this.workYear = workYear;
    }

    public String getMemo() {
        return memo;
    }

    public void setMemo(String memo) {
        this.memo = memo;
    }

    public ScheDayData getDay1() {
        return day1;
    }

    public void setDay1(ScheDayData day1) {
        this.day1 = day1;
    }

    public ScheDayData getDay2() {
        return day2;
    }

    public void setDay2(ScheDayData day2) {
        this.day2 = day2;
    }

    public ScheDayData getDay3() {
        return day3;
    }

    public void setDay3(ScheDayData day3) {
        this.day3 = day3;
    }

    public ScheDayData getDay4() {
        return day4;
    }

    public void setDay4(ScheDayData day4) {
        this.day4 = day4;
    }

    public ScheDayData getDay5() {
        return day5;
    }

    public void setDay5(ScheDayData day5) {
        this.day5 = day5;
    }

    public ScheDayData getDay6() {
        return day6;
    }

    public void setDay6(ScheDayData day6) {
        this.day6 = day6;
    }

    public ScheDayData getDay7() {
        return day7;
    }

    public void setDay7(ScheDayData day7) {
        this.day7 = day7;
    }


}